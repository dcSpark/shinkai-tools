{
    "name": "Gmail Developer API",
    "homepage": "https://shinkai.com",
    "description": "Tool to make requests to the Gmail Developer API using OAuth for authentication.",
    "author": "@@localhost.sep-shinkai",
    "version": "1.0.0",
    "keywords": [
        "gmail",
        "api",
        "request",
        "oauth"
    ],
    "runner": "any",
  "operating_system": ["linux", "macos", "windows"],
  "tool_set": "",
  "configurations": {
        "type": "object",
        "properties": {},
        "required": []
    },
    "parameters": {
        "type": "object",
        "properties": {
            "url": { 
              "type": "string", 
              "description": "Gmail API endpoint URL (e.g., 'https://gmail.googleapis.com/gmail/v1/users/me/messages')"
            },
            "method": { 
              "type": "string", 
              "description": "HTTP method: GET (read), POST (send), PUT (update), DELETE (trash/delete messages)"
            },
            "body": { 
              "type": "string", 
              "description": "JSON string for request body. For emails, must include proper MIME format",
              "nullable": true 
            },
            "query_params": { 
              "type": "string", 
              "description": "URL query parameters as JSON object string (e.g., '{\"q\": \"subject:important\"}')",
              "nullable": true 
            },
            "headers": { 
              "type": "string", 
              "description": "Custom headers as 'key: value' pairs, one per line. OAuth headers added automatically",
              "nullable": true 
            }
        },
        "required": [
            "url",
            "method"
        ]
    },
    "result": {
        "type": "object",
        "properties": {
            "status": { 
              "type": "number", 
              "description": "HTTP status code (2xx=success, 4xx=client error, 5xx=server error)"
            },
            "statusText": { 
              "type": "string", 
              "description": "HTTP status message (e.g., 'OK', 'Message Sent', 'Not Found')"
            },
            "data": { 
              "type": "any", 
              "description": "Response payload as parsed JSON. Contains email data, labels, or operation results"
            }
        },
        "required": [
            "status",
            "statusText",
            "data"
        ]
    },
    "sqlTables": [],
    "sqlQueries": [],
    "tools": [],
    "oauth": [
        {
            "name": "Gmail",
            "version": "2.0",
            "authorizationUrl": "https://accounts.google.com/o/oauth2/v2/auth",
            "redirectUrl": "https://secrets.shinkai.com/redirect",
            "tokenUrl": "https://oauth2.googleapis.com/token",
            "clientId": "",
            "clientSecret": "",
            "scopes": [
              "https://www.googleapis.com/auth/gmail.readonly",
              "https://www.googleapis.com/auth/gmail.modify",
              "https://www.googleapis.com/auth/gmail.send",
              "https://www.googleapis.com/auth/gmail.compose",
              "https://www.googleapis.com/auth/userinfo.email",
              "https://www.googleapis.com/auth/userinfo.profile"
            ],
            "responseType": "code",
            "refreshToken": "",
            "pkceType": "",
            "request_token_auth_header": "",
            "request_token_content_type": ""
          }
    ]
}
