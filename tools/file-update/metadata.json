{
    "id": "file-update",
    "name": "Update File with Prompt",
    "version": "1.0.0",
    "description": "Applies a prompt to the file contents.",
    "author": "Shinkai",
    "keywords": [
      "file",
      "update",
      "prompt",
      "text"
    ],
    "configurations": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "parameters": {
      "type": "object",
      "properties": {
        "path": {
          "type": "string",
          "description": "The path of the file to update."
        },
        "prompt": {
          "type": "string",
          "description": "The prompt to apply to the file contents."
        }
      },
      "required": [
        "path",
        "prompt"
      ]
    },
    "result": {
      "type": "object",
      "properties": {
        "new_file_content": {
          "type": "string",
          "description": "The path of the file that was updated."
        },
        "message": {
          "type": "string",
          "description": "The message returned from the tool."
        }
      },
      "required": [
        "message"
      ]
    },
    "sqlTables": [],
    "sqlQueries": [],
