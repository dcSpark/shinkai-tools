{
    "name": "Write File Contents",
    "homepage": "https://github.com/dcSpark/shinkai-tools/blob/main/tools/file-write/README.md",
    "version": "1.0.0",
    "description": "Writes the text contents of a file to the given path.",
    "author": "Shinkai",
    "keywords": [
      "file",
      "write",
      "save",
      "content",
      "text"
    ],
    "runner": "any",
  "operating_system": ["linux", "macos", "windows"],
  "tool_set": "",
  "configurations": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "parameters": {
      "type": "object",
      "properties": {
        "path": {
          "type": "string",
          "description": "Absolute or relative path to target file. Creates parent directories if needed"
        },
        "content": {
          "type": "string",
          "description": "Text content to write. Overwrites existing file content if file exists"
        }
      },
      "required": [
        "path",
        "content"
      ]
    },
    "result": {
      "type": "object",
      "properties": {
        "message": {
          "type": "string",
          "description": "Status message indicating success or details of any errors encountered"
        }
      },
      "required": [
        "message"
      ]
    },
    "sqlTables": [],
    "sqlQueries": [],
    "tools": [],
    "oauth": null
  }
