{
    "name": "Math Expression Evaluator",
    "homepage": "<https://github.com/dcSpark/shinkai-tools/blob/main/tools/math-exp/README.md>",
    "description": "Parses and evaluates mathematical expressions. It’s a safer and more math-oriented alternative to using JavaScript’s eval function for mathematical expressions.",
    "author": "Shinkai",
    "keywords": [
      "math",
      "expr-eval",
      "shinkai"
    ],
    "version": "1.0.0",
    "configurations": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "parameters": {
      "type": "object",
      "properties": {
        "expression": {
          "type": "string",
          "description": "The mathematical expression to evaluate (e.g., '2 + 2 * 3')"
        }
      },
      "required": [
        "expression"
      ]
    },
    "result": {
      "type": "object",
      "properties": {
        "result": {
          "type": "string",
          "description": "The evaluated result of the mathematical expression"
        }
      },
      "required": [
        "result"
      ]
    }
  }
