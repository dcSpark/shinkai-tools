{
    "id": "shinkai-tool-coinbase-get-balance",
    "version": "1.0.0",
    "name": "Shinkai: Coinbase Balance Getter",
    "description": "Tool for getting the balance of a Coinbase wallet after restoring it",
    "author": "Shinkai",
    "keywords": [
      "coinbase",
      "balance",
      "shinkai"
    ],
    "configurations": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "privateKey": {
          "type": "string"
        },
        "walletId": {
          "type": "string",
          "nullable": true
        },
        "useServerSigner": {
          "type": "string",
          "nullable": true
        }
      },
      "required": [
        "name",
        "privateKey"
      ]
    },
    "parameters": {
      "type": "object",
      "properties": {
        "walletId": {
          "type": "string",
          "nullable": true
        }
      },
      "required": []
    },
    "result": {
      "type": "object",
      "properties": {
        "message": {
          "type": "string"
        },
        "balances": {
          "type": "object",
          "additionalProperties": {
            "type": "number"
          },
          "required": []
        }
      },
      "required": [
        "message",
        "balances"
      ]
    }
  }