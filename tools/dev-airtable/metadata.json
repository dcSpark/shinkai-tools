{
    "name": "Airtable Developer API",
    "homepage": "",
    "description": "Tool to make requests to the Airtable Developer API using OAuth for authentication.",
    "author": "@@localhost.sep-shinkai",
    "version": "1.0.0",
    "keywords": [
        "airtable",
        "api",
        "oauth"
    ],
    "runner": "any",
  "operating_system": ["linux", "macos", "windows"],
  "tool_set": "",
  "configurations": {
        "type": "object",
        "properties": {},
        "required": []
    },
    "parameters": {
        "type": "object",
        "properties": {
            "url": { 
              "type": "string", 
              "description": "Full Airtable API endpoint URL (e.g., 'https://api.airtable.com/v0/base_id/table_name')"
            },
            "method": { 
              "type": "string", 
              "description": "HTTP method: GET (read), POST (create), PUT (update), or DELETE (remove)"
            },
            "body": { 
              "type": "string", 
              "description": "JSON string for request body. Required for POST/PUT. Must be valid JSON",
              "nullable": true 
            },
            "query_params": { 
              "type": "string", 
              "description": "URL query parameters as JSON object string (e.g., '{\"maxRecords\": 100}')",
              "nullable": true 
            },
            "headers": { 
              "type": "string", 
              "description": "Custom headers as 'key: value' pairs, one per line. OAuth headers added automatically",
              "nullable": true 
            }
        },
        "required": [
            "url",
            "method"
        ]
    },
    "result": {
        "type": "object",
        "properties": {
            "status": { 
              "type": "number", 
              "description": "HTTP status code (2xx=success, 4xx=client error, 5xx=server error)"
            },
            "statusText": { 
              "type": "string", 
              "description": "HTTP status message (e.g., 'OK', 'Bad Request', 'Not Found')"
            },
            "data": { 
              "type": "any", 
              "description": "Response payload as parsed JSON. Structure varies by endpoint and method"
            }
        },
        "required": [
            "status",
            "statusText",
            "data"
        ]
    },
    "sqlTables": [],
    "sqlQueries": [],
    "tools": [],
    "oauth": [
        {
            "name": "Airtable",
            "version": "2.0",
            "authorizationUrl": "https://airtable.com/oauth2/v1/authorize",
            "redirectUrl": "https://secrets.shinkai.com/redirect",
            "tokenUrl": "https://airtable.com/oauth2/v1/token",
            "clientId": "",
            "clientSecret": "",
            "scopes": [
              "data.records:read",
              "data.records:write",
              "schema.bases:read",
              "schema.bases:write"
            ],
            "responseType": "code",
            "refreshToken": "",
            "pkceType": "S256",
            "requestTokenAuthHeader": "basic",
            "requestTokenContentType": "application/x-www-form-urlencoded"
          }
    ]
}
