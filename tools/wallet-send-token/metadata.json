{
  "name": "Send ERC20 Tokens",
  "homePage": "",
  "description": "Sends ERC20 tokens.",
  "version": "1.0.0",
  "keywords": [
    "wallet",
    "tokens",
    "erc20",
    "blockchain"
  ],
  "configurations": {
    "type": "object",
    "properties": {
      "chain": {
        "type": "string",
        "description": "The blockchain network",
        "order": 1
      },
      "privateKey": {
        "type": "string",
        "description": "The private key of the wallet",
        "order": 2
      },
      "rpcURL": {
        "type": "string",
        "description": "The RPC URL of the blockchain",
        "order": 3
      }
    },
    "required": [
      "chain",
      "privateKey",
      "rpcURL"
    ]
  },
  "parameters": {
    "type": "object",
    "properties": {
      "toAddress": {
        "type": "string",
        "description": "The address to send tokens to",
        "order": 1
      },
      "contractAddress": {
        "type": "string",
        "description": "The contract address of the token",
        "order": 2
      },
      "amount": {
        "type": "number",
        "description": "The amount of tokens to send",
        "order": 3
      }
    },
    "required": [
      "toAddress",
      "contractAddress",
      "amount"
    ]
  },
  "result": {
    "type": "object",
    "properties": {
      "receipt": {
        "type": "object",
        "properties": {
          "transactionHash": {
            "type": "string",
            "description": "Transaction hash of the executed transaction",
            "order": 1
          },
          "status": {
            "type": "string",
            "description": "Status of the transaction",
            "order": 2
          },
          "gasUsed": {
            "type": "number",
            "description": "Amount of gas used for the transaction",
            "order": 3
          },
          "gasPrice": {
            "type": "number",
            "description": "Gas price at the transaction time",
            "order": 4
          },
          "from": {
            "type": "string",
            "description": "Sender's address",
            "order": 5
          },
          "to": {
            "type": "string",
            "description": "Receiver's address",
            "order": 6
          }
        },
        "required": [
          "transactionHash",
          "status",
          "gasUsed",
          "gasPrice",
          "from"
        ]
      }
    },
    "required": [
      "receipt"
    ]
  },
  "sqlTables": [],
  "sqlQueries": [],
  "oauth": [],
  "runner": "any",
  "operating_system": [
    "linux",
    "macos",
    "windows"
  ],
  "tool_set": ""
}