{
    "name": "Google Search",
    "homepage": "https://github.com/dcSpark/shinkai-tools/blob/main/tools/google-search/README.md",
    "description": "This function takes a question as input and returns a comprehensive answer, along with the sources and statements used to generate the answer.",  
    "author": "Shinkai",
    "version": "1.0.0",
    "keywords": [
      "search",
      "answer generation",
      "fact extraction",
      "wikipedia",
      "google"
    ],  
    "runner": "any",
  "operating_system": ["linux", "macos", "windows"],
  "tool_set": "",
  "configurations": [],
    "parameters": {
      "type": "object",
      "properties": {
        "query": {
          "type": "string",
          "description": "Search terms to find web pages. Keywords separated by spaces, quotes for exact phrases"
        },
        "num_results": {
          "type": "number",
          "description": "Maximum number of search results to return (1-100). Default: 10",
          "default": 10
        }
      },
      "required": [
        "query"
      ]
    },
    "result": {
      "properties": {
        "query": {
          "type": "string",
          "description": "Search query that was used to find these results"
        },
        "results": {
            "type": "array",
            "description": "Array of web pages matching search criteria, ordered by relevance",
            "items": {
                "type": "object",
                "properties": {
                    "title": {
                      "type": "string",
                      "description": "Page title or headline"
                    },
                    "url": {
                      "type": "string",
                      "description": "Full URL to the web page"
                    },
                    "description": {
                      "type": "string",
                      "description": "Brief excerpt or summary of page content"
                    }
                },
                "required": ["title", "url", "description"]
            }
        }
    },
      "required": [
        "query",
        "results"
      ],
      "type": "object"
    },
    "sqlTables": [],  
    "sqlQueries": [],
    "tools": [
    ]
  }
