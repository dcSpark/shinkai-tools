{
    "name": "Play Crypto 2048",
    "homePage": "",
    "description": "Automatically play Crypto 2048",
    "author": "@@official.shinkai",
    "version": "1.0.0",
    "keywords": [],
    "runner": "any",
  "operating_system": ["linux", "macos", "windows"],
  "tool_set": "",
  "configurations": {
      "type": "object",
      "properties": {
        "OPENAI_KEY": {
          "type": "string",
          "description": "OpenAI API key for GPT-4 model that controls game strategy"
        },
        "domSettleTimeoutMs": {
          "type": "number",
          "description": "Milliseconds to wait for UI updates between moves. Default: 100",
          "default": 100
        },
        "verbose": {
          "type": "number",
          "description": "Logging level (0-2). Higher values show more debug info. Default: 1",
          "default": 1
        }
      },
      "required": ["OPENAI_KEY"]
    },
    "parameters": {
      "type": "object",
      "properties": {
        "minMoveDelayMs": {
          "type": "number",
          "description": "Minimum milliseconds between moves. Default: 100",
          "default": 100
        },
        "maxMoveDelayMs": {
          "type": "number",
          "description": "Maximum milliseconds between moves. Default: 300",
          "default": 300
        },
        "maxMoves": {
          "type": "number",
          "description": "Stop after this many moves. Default: infinite",
          "default": 0
        },
        "username": {
          "type": "string",
          "description": "Username for game login. Optional if already logged in"
        },
        "password": {
          "type": "string",
          "description": "Password for game login. Optional if already logged in"
        }
      },
      "required": []
    },
    "result": {
      "type": "object",
      "properties": {
        "score": {
          "type": "number",
          "description": "Current game score achieved by the AI player"
        },
        "highestTile": {
          "type": "number",
          "description": "Value of the highest numbered tile on the game board"
        },
        "grid": {
          "type": "array",
          "description": "4x4 matrix of tile values. Empty cells are 0, other cells contain powers of 2"
        }
      },
      "required": ["score", "highestTile", "grid"]
    },
    "sqlTables": [],
    "sqlQueries": [],
    "tools": [
      "local:::__official_shinkai:::shinkai_typescript_unsafe_processor"
    ],
    "oauth": []
  }
