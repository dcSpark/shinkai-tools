{
  "name": "@shinkai_protocol/shinkai-tool-meme-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/shinkai-tool-meme-generator/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno bundle src/index.ts dist/index.js",
        "cwd": "apps/shinkai-tool-meme-generator"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno test --no-check --allow-all ./src/**/*.test.ts",
        "cwd": "apps/shinkai-tool-meme-generator"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno lint",
        "cwd": "apps/shinkai-tool-meme-generator"
      }
    }
  },
  "tags": []
}
