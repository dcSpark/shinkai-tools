{
    "id": "shinkai-tool-coinbase-get-my-address",
    "version": "1.0.0",
    "name": "Shinkai: Coinbase My Address Getter",
    "description": "Tool for getting the default address of a Coinbase wallet",
    "author": "Shinkai",
    "keywords": [
      "coinbase",
      "address",
      "shinkai"
    ],
    "configurations": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "privateKey": {
          "type": "string"
        },
        "walletId": {
          "type": "string",
          "nullable": true
        },
        "useServerSigner": {
          "type": "string",
          "nullable": true
        }
      },
      "required": [
        "name",
        "privateKey"
      ]
    },
    "parameters": {
      "type": "object",
      "properties": {
        "walletId": {
          "type": "string",
          "nullable": true
        }
      },
      "required": []
    },
    "result": {
      "type": "object",
      "properties": {
        "address": {
          "type": "string"
        }
      },
      "required": [
        "address"
      ]
    }
  }